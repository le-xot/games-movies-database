{
  "name": "backend",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "bunx --bun nest build",
    "start": "bun run dist/main.js",
    "dev": "bunx --bun nest start --watch",
    "seed": "bun run ./prisma/seed.js",
    "prestart": "bunx prisma migrate deploy && bun seed",
    "predev": "bun seed",
    "prisma": "bunx prisma"
  },
  "dependencies": {
    "@nestjs/common": "10.4.7",
    "@nestjs/core": "10.4.7",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.7",
    "@nestjs/platform-socket.io": "11.1.6",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "8.0.2",
    "@nestjs/websockets": "11.1.6",
    "@prisma/client": "6.2.1",
    "@scalar/nestjs-api-reference": "0.3.167",
    "@soundify/web-api": "1.1.5",
    "axios": "1.11.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.7",
    "dotenv": "16.4.5",
    "envalid": "8.0.0",
    "http-proxy-agent": "7.0.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "socket.io": "4.8.1",
    "socks-proxy-agent": "8.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.7",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.7",
    "@types/bun": "^1.3.1",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/supertest": "6.0.2",
    "jest": "29.7.0",
    "prisma": "6.2.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
