{
  "name": "backend",
  "type": "module",
  "version": "2.0.1",
  "private": true,
  "scripts": {
    "build": "bunx --bun tsc --noEmit",
    "start": "bun run ./src/main.ts",
    "dev": "bun run --watch --no-clear-screen ./src/main.ts",
    "seed": "bun run ./prisma/seed.js",
    "prestart": "bun prisma migrate deploy && bun seed",
    "predev": "bun seed",
    "prisma": "bunx prisma"
  },
  "dependencies": {
    "@nestjs/common": "10.4.7",
    "@nestjs/core": "10.4.7",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.7",
    "@nestjs/platform-socket.io": "11.1.6",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "8.0.2",
    "@nestjs/websockets": "11.1.6",
    "@prisma/client": "6.2.1",
    "@scalar/nestjs-api-reference": "0.3.167",
    "@soundify/web-api": "1.1.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.7",
    "envalid": "8.0.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "sharp": "0.34.5",
    "socket.io": "4.8.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "5.0.0",
    "prisma": "6.2.1",
    "source-map-support": "0.5.21",
    "typescript": "5.9.3"
  }
}
